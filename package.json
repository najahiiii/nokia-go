{
  "name": "nokia-router",
  "version": "1.0.7",
  "description": "Nokia FastMile 5G Gateway 2 custom Web UI",
  "main": "index.js",
  "scripts": {
    "build:css": "tailwindcss -c tailwind.config.js -i tailwind.css -o templates/assets/tailwind.css --minify",
    "build:js": "esbuild templates/script/nokia.js --minify --format=iife --global-name=NokiaApp --outfile=templates/script/nokia.min.js && esbuild templates/script/tabs.js --minify --format=iife --global-name=NokiaTabs --outfile=templates/script/tabs.min.js"
  },
  "keywords": [],
  "author": "Ahmad Thoriq Najahi",
  "license": "Apache-2.0",
  "type": "commonjs",
  "devDependencies": {
    "esbuild": "^0.21.5",
    "tailwindcss": "^3.4.16"
  }
}
